class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int>st;
        int N=nums.size();
        for(int i=0; i<N; i++){
            st.insert(nums[i]);
        }

        int index=0;
        for(auto it=st.begin(); it!=st.end(); it++){
            nums[index]=*it;
            index++;
        }

        return st.size();
    }
};
